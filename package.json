{
  "name": "fipco",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "watch:tailwindcss": "tailwindcss -i src/index.css -o public/style.css --minify -w",
    "build:tailwindcss": "tailwindcss -i  src/index.css -o public/style.css --minify",
    "dev": "vite",
    "start": "concurrently --kill-others \"npm run dev\" \" npm run watch:tailwindcss\" ",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@material-tailwind/react": "^2.1.9",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.40",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropdown": "^1.11.0",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.25.1",
    "react-toastify": "^10.0.5"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.3.4"
  }
}
